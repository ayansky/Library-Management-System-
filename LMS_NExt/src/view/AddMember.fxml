<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="392.0" prefWidth="417.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddMemberController">
   <children>
      <Label layoutX="139.0" layoutY="14.0" prefHeight="32.0" prefWidth="144.0" text="Add Member">
         <font>
            <Font size="22.0" />
         </font></Label>
      <Button fx:id="saveMember" layoutX="231.0" layoutY="308.0" mnemonicParsing="false" onAction="#saveMember" prefHeight="32.0" prefWidth="130.0" text="Save" />
      <VBox layoutX="63.0" layoutY="70.0" prefHeight="227.0" prefWidth="303.0">
         <children>
            <TextField fx:id="firstname" promptText="Enter First Name">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="lastname" promptText="Enter Last Name">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="memberId" promptText="Enter User ID">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </TextField>
            <PasswordField fx:id="password" promptText="Enter Password">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </PasswordField>
            <TextField fx:id="confirm" promptText="Confirm Password">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </TextField>
         </children>
      </VBox>
      <Label fx:id="messageLabel" layoutX="148.0" layoutY="52.0" text="Member information" />
      <Button fx:id="cancel" layoutX="73.0" layoutY="308.0" mnemonicParsing="false" onAction="#cancelAdd" prefHeight="32.0" prefWidth="121.0" text="Cancel" />
   </children>
</AnchorPane>
